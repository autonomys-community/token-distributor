{
  "name": "autonomys-token-distributor",
  "version": "1.0.0",
  "description": "A robust token distribution tool for the Autonomys Network",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "distribute": "npm run build && node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts tests/**/*.ts",
    "format:check": "prettier --check src/**/*.ts tests/**/*.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "autonomys",
    "blockchain",
    "token",
    "distribution",
    "crypto"
  ],
  "author": "Counterpoint Software",
  "license": "MIT",
  "dependencies": {
    "@autonomys/auto-consensus": "^1.0.0",
    "@autonomys/auto-utils": "^1.5.17",
    "@polkadot/api": "^15.10.2",
    "@polkadot/keyring": "^13.5.6",
    "@polkadot/util-crypto": "^13.5.6",
    "chalk": "^5.3.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.12",
    "joi": "^17.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.5",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
